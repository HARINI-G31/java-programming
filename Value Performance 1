import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    int b =sc.nextInt();
    float a=sc.nextFloat();
    System.out.println(b);
    System.out.printf("%.2f",a);
    }
}
