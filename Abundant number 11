import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in=new Scanner(System.in);
        int a =in.nextInt();
        int i,sum=0;
        for(i=1;i<a;i++){
            if(a%i==0){
                sum=sum+i;
            }
        }
      
        if(a<sum){
            System.out.println("Abundant Number"); 
        }
        else{
            System.out.println("Not Abundant Number");
        }
    }
}
