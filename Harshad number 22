import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
    Scanner in =new Scanner(System.in);
        int a =in.nextInt();
        int b=a;
        int sum=0;
            while(a>0){
                int rem=a%10;
                sum=sum+rem;
                a=a/10;
            }
        if(b%sum==0){
          System.out.println("Harshad Number");
              
        }
        else{
            System.out.println("Not Harshad Number");
            
        }
            
            
        }
    }
