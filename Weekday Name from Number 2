import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
         Scanner sc = new Scanner(System.in);
        int day = sc.nextInt();
        String Day;
        switch (day) {
            case 1: 
                Day = "Monday"; 
                System.out.println(Day);
                break;
            case 2: 
                Day = "Tuesday";   
                System.out.println(Day);
                break;
            case 3:
                Day = "Wednesday";
                System.out.println(Day);
                break;
            case 4: 
                Day = "Thursday";  
                System.out.println(Day);
                break;
            case 5: 
                Day = "Friday"; 
                System.out.println(Day);
                break;
            case 6: 
                Day = "Saturday";  
                System.out.println(Day);
                break;
            case 7: 
                Day = "Sunday";
                System.out.println(Day);
                break;
            default:
                 System.out.println("Enter a valid Input");
        }
         
       
}
}
